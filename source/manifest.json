{
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/notification.css" ],
      "js": [ "js/jquery-1.7.2.min.js", "js/mousetrap-1.4.6.min.js", "coach-utils.js", "webparsers.js", "coach-gmail.js" ],
      "matches": [ "*://mail.google.com/*"],
      "run_at": "document_end"
   },
   {
      "all_frames": true,
      "css": [ "css/notification.css" ],
      "js": [ "js/jquery-1.7.2.min.js", "coach-utils.js", "customBinder.js",
      "webparsers.js", "coach-github.js" ],
      "matches": [ "*://github.com/*"],
      "run_at": "document_end"
   }
   
   
   
   ],
"browser_action": {
      "default_icon": {
         "16": "whistle-icon16.png",
         "32": "whistle-icon32.png"
      },
      "default_popup": "popup.html",
      "default_title": "Some message here"
   },

   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Gives a reminder for keyboard shortcuts in an intelligent manner. ",
   "icons": {
      "128": "whistle-icon128.png"
   },
   "manifest_version": 2,
   "name": "Keyboard Shortcut Coach",
   "permissions": [ "*://mail.google.com/*", "*://github.com/*",

      "storage"
    ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1",
   "web_accessible_resources": [ "images/close_information.png" ]
}
